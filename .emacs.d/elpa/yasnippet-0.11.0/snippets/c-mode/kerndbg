# -*- mode: snippet -*-
# name: kerndbg
# contributor: HardDie
# key: kerndbg
# --
/* DEBUG */
#define __my_print(__fmt, ...) printk(KERN_INFO __fmt, ##__VA_ARGS__)
#define __log_print(__fmt, ...) __my_print("dbg  "__fmt": %s():[%d]\n", ##__VA_ARGS__, __FUNCTION__, __LINE__)
#define DPRINT(__flag, __fmt, ...) \
	if (__flag) __log_print(__fmt, ##__VA_ARGS__);
/* DEBUG */
